<local:UserControlPlus x:Class="FESScript2.Graphics.UserControls.SubUserControls.Dots"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FESScript2.Graphics.UserControls"
             xmlns:UserControls="clr-namespace:FESScript2.Graphics.UserControls"
             xmlns:SubUserControls="clr-namespace:FESScript2.Graphics.UserControls.SubUserControls"
             mc:Ignorable="d" 
             d:DesignHeight="15" d:DesignWidth="15" RenderTransformOrigin="0.5,0.5">
    <Grid MouseDown="Grid_MouseDown" x:Name="grid" RenderTransformOrigin="0.5,0.5">
        <Ellipse x:Name="ellipse" Stroke="Gray" StrokeThickness="2" Width="15" Height="15"/>
    </Grid>
</local:UserControlPlus>
